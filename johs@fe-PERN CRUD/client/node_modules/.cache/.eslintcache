r=\"contact\" className='form-control  my-3'\n            style={{borderRadius:\"10px 20px 30px 40px\", width:\"650px\"}}\n            value={contact}\n            onChange = {e => onChange(e)}\n            />\n\n            <div className=\"d-flex mt-3\" >\n            <button type=\"submit\" value=\"send\" onClick={onSubmitForm}\n            className=\"form-control btn btn-outline-primary\"\n            style={{ marginTop: \"20px\", width:\"150px\",borderRadius:\" 20px 40px\",\n            fontSize:\"19px\",}}>Add</button>\n\n            <button type=\"submit\" value=\"cancel\" \n            onClick={onCancelForm}\n            className=\"form-control btn btn-outline-secondary ml-5\"\n            style={{ marginTop: \"19px\", width:\"150px\",borderRadius:\" 20px  40px\",\n            fontSize:\"20px\",}}>Cancel</button>\n\n             </div>\n             <ToastContainer />\n             </div>\n             \n             </div>\n\n            </form>\n\n            </div>\n            \n           \n   );\n}\nexport default Addcontact;","/home/gambler/Desktop/Project/pern-todo-ic/johs@fe-PERN CRUD/client/src/components/editcontact.js",["42","43"],[],"/home/gambler/Desktop/Project/pern-todo-ic/johs@fe-PERN CRUD/client/src/components/listcontacts.js",["44","45","46","47","48","49","50","51","52"],[],"/home/gambler/Desktop/Project/pern-todo-ic/johs@fe-PERN CRUD/client/src/components/Footer.js",[],[],"/home/gambler/Desktop/Project/pern-todo-ic/johs@fe-PERN CRUD/client/src/components/Contactcard.js",["53"],[],"\n\nimport avatar from '../images/avatar.png';\nimport { Link, useParams } from 'react-router-dom';\n\nconst contactCard = ({contacts}) =>{\n\n      //  const { id } = useParams();\n    \n      return(\n        <div>\n        <div class=\"card\" style={{width:\"400px\"}}>\n            <img class=\"card-img-top\" src={avatar}  alt={avatar}/>\n            <div class=\"card-body\">\n            <h4 class=\"card-title\">{contacts.user_name}</h4>\n            <p c